<?xml version="1.0" encoding="utf-8"?>
<animated-selector android:constantSize="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_pressed="true">
        <bitmap android:gravity="center" android:src="@drawable/scrubber_control_off_mtrl_alpha" android:tint="@color/oneplus_seekbar_activated_color" android:alpha="?android:disabledAlpha" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:gravity="center" android:src="@drawable/scrubber_control_off_mtrl_alpha" android:tint="@color/oneplus_seekbar_normal_color" android:alpha="?android:disabledAlpha" />
    </item>
    <item android:state_pressed="true" android:id="@id/pressed">
        <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_005" android:tint="@color/oneplus_seekbar_activated_color" />
    </item>
    <item android:id="@id/not_pressed">
        <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_000" android:tint="@color/oneplus_seekbar_activated_color" />
    </item>
    <transition android:toId="@id/pressed" android:fromId="@id/not_pressed">
        <animation-list>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_000" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_001" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_002" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_003" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_004" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_005" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
        </animation-list>
    </transition>
    <transition android:toId="@id/not_pressed" android:fromId="@id/pressed">
        <animation-list>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_000" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_001" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_002" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_003" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_004" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_005" android:tint="@color/oneplus_seekbar_activated_color" />
            </item>
        </animation-list>
    </transition>
</animated-selector>