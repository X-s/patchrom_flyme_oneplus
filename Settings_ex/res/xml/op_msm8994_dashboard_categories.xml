<?xml version="1.0" encoding="utf-8"?>
<dashboard-categories
  xmlns:android="http://schemas.android.com/apk/res/android">
    <dashboard-category android:id="@id/wireless_section" android:title="@string/oneplus_header_category_wireless_networks">
        <dashboard-tile android:icon="@drawable/op_ic_double_crad_manager" android:id="@id/sim_settings_and_mobile_network_settings" android:title="@string/oneplus_sim_and_mobile_settings_title">
            <intent android:action="oneplus.intent.action.SIM_AND_NETWORK_SETTINGS" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_wireless" android:id="@id/wifi_settings" android:title="@string/oneplus_wifi_settings_title" android:fragment="com.android.settings_ex.wifi.WifiSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_bluetooth2" android:id="@id/bluetooth_settings" android:title="@string/oneplus_bluetooth_settings_title" android:fragment="com.android.settings_ex.bluetooth.BluetoothSettings" />
        <dashboard-tile android:id="@id/operator_settings" android:fragment="com.android.settings_ex.WirelessSettings">
            <intent android:action="com.android.settings_ex.OPERATOR_APPLICATION_SETTING" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/op_ic_settings_more" android:id="@id/wireless_settings" android:title="@string/oneplus_radio_controls_title" android:fragment="com.android.settings_ex.WirelessSettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/device_section" android:title="@string/oneplus_header_display_sound_settings_title">
        <dashboard-tile android:icon="@drawable/ic_settings_zenmode" android:id="@id/zen_mode" android:title="@string/oneplus_three_key_zenmod" android:fragment="com.oneplus.settings.notification.OPThreeKeyAndZenmode" />
        <dashboard-tile android:icon="@drawable/ic_settings_notifications" android:id="@id/notification_settings" android:title="@string/oneplus_notification_settings" android:fragment="com.oneplus.settings.notification.OPNotificationSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_display" android:id="@id/display_settings" android:title="@string/oneplus_display_settings" android:fragment="com.oneplus.settings.OPDisplaySettings" />
        <dashboard-tile android:id="@id/manufacturer_settings" android:fragment="com.android.settings_ex.WirelessSettings">
            <intent android:action="com.android.settings_ex.MANUFACTURER_APPLICATION_SETTING" />
        </dashboard-tile>
    </dashboard-category>
    <dashboard-category android:id="@id/private_safety_section" android:title="@string/oneplus_private_safety_title">
        <dashboard-tile android:icon="@drawable/ic_settings_location" android:id="@id/location_settings" android:title="@string/oneplus_location_settings_title" android:fragment="com.android.settings_ex.location.LocationSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_fingerprint" android:id="@id/application_permisstion_manage" android:title="@string/oneplus_fingerprint_password_manage_title" android:fragment="com.oneplus.settings.security.OPSecuritySettings" />
        <dashboard-tile android:icon="@drawable/op_ic_notification_manager" android:id="@id/fingerprint_password_manage_settings" android:title="@string/application_notification_title" android:fragment="com.oneplus.settings.notification.OPNotificationAppList" />
        <dashboard-tile android:icon="@drawable/ic_settings_applications_permisstion" android:id="@id/security_settings" android:title="@string/oneplus_applications_permission_settings" android:fragment="com.oneplus.settings.security.OPApplicationSecuritySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_applications" android:id="@id/application_settings" android:title="@string/oneplus_applications_permission_settings" android:fragment="com.android.settings_ex.applications.ManageApplications" />
        <dashboard-tile android:icon="@drawable/ic_settings_accounts" android:id="@id/account_settings" android:title="@string/oneplus_account_settings_title" android:fragment="com.android.settings_ex.accounts.AccountSettings" />
        <dashboard-tile android:icon="@drawable/op_ic_settings_backup" android:id="@id/privacy_settings" android:title="@string/master_clear_title" android:fragment="com.android.settings_ex.PrivacySettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/system_section" android:title="@string/oneplus_header_other_settings_title">
        <dashboard-tile android:icon="@drawable/op_ic_intelligent_assistant" android:id="@id/iq_acc_settings" android:title="@string/oneplus_iq_settings_title" android:fragment="com.oneplus.settings.iq.OPIQSettings" />
        <dashboard-tile android:icon="@drawable/op_ic_settings_battery" android:id="@id/battery_settings" android:title="@string/power_usage_summary_title" android:fragment="com.android.settings_ex.fuelgauge.PowerUsageSummary" />
        <dashboard-tile android:icon="@drawable/ic_settings_date_time" android:id="@id/date_time_settings" android:title="@string/oneplus_date_and_time_settings_title" android:fragment="com.android.settings_ex.DateTimeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_cn_language" android:id="@id/language_settings" android:title="@string/oneplus_language_settings" android:fragment="com.android.settings_ex.inputmethod.InputMethodAndLanguageSettings" />
        <dashboard-tile android:icon="@drawable/op_ic_other_settings" android:id="@id/onepluse_other_development_settings" android:title="@string/oneplus_others_settings_title" android:fragment="com.oneplus.settings.others.OPOthersSettings" />
        <dashboard-tile android:icon="@drawable/oneplus_noviceguide" android:id="@id/op_noviceguide" android:title="@string/oneplus_noviceguide">
            <intent android:action="com.oneplus.noviceteaching.NoViceTeachingActivity" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/op_ic_update_settings" android:id="@id/onepluse_system_upgrade_settings" android:title="@string/system_update_settings_list_item_title">
            <intent android:action="android.intent.action.CheckUpdate" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_development" android:id="@id/development_settings" android:title="@string/oneplus_development_settings_title" android:fragment="com.android.settings_ex.DevelopmentSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_about" android:id="@id/about_settings" android:title="@string/oneplus_about_settings" android:fragment="com.oneplus.settings.OPDeviceInfoSettings" />
    </dashboard-category>
</dashboard-categories>