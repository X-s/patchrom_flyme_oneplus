<?xml version="1.0" encoding="utf-8"?>
<dashboard-categories
  xmlns:android="http://schemas.android.com/apk/res/android">
    <dashboard-category android:id="@id/wireless_section" android:title="@string/header_category_wireless_networks" android:key="@string/category_key_wireless">
        <dashboard-tile android:icon="@drawable/ic_settings_wireless" android:id="@id/wifi_settings" android:title="@string/wifi_settings_title" android:fragment="com.android.settings_ex.wifi.WifiSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_bluetooth" android:id="@id/bluetooth_settings" android:title="@string/bluetooth_settings_title" android:fragment="com.android.settings_ex.bluetooth.BluetoothSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_sim" android:id="@id/sim_settings" android:title="@string/op_sim_cord">
            <intent android:action="oneplus.intent.action.SIM_AND_NETWORK_SETTINGS" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_data_usage" android:id="@id/data_usage_settings" android:title="@string/data_usage_summary_title">
            <intent android:action="com.oneplus.security.action.USAGE_DATA_SUMMARY" />
        </dashboard-tile>
        <dashboard-tile android:id="@id/operator_settings" android:fragment="com.android.settings_ex.WirelessSettings">
            <intent android:action="com.android.settings_ex.OPERATOR_APPLICATION_SETTING" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_more" android:id="@id/wireless_settings" android:title="@string/radio_controls_title" android:fragment="com.android.settings_ex.WirelessSettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/device_section" android:title="@string/header_category_device" android:key="@string/category_key_device">
        <dashboard-tile android:icon="@drawable/ic_settings_home" android:id="@id/home_settings" android:title="@string/home_settings" android:fragment="com.android.settings_ex.HomeSettings" />
        <dashboard-tile android:icon="@drawable/ic_do_not_disturb" android:id="@id/do_not_disturb_settings" android:title="@string/oneplus_three_key_zenmod" android:fragment="com.oneplus.settings.notification.OPNotificationAndNotdisturb" />
        <dashboard-tile android:icon="@drawable/ic_settings_buttons" android:id="@id/buttons_settings" android:title="@string/op_buttons_settings" android:fragment="com.oneplus.settings.OPButtonsSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_gestures" android:id="@id/gestures_settings" android:title="@string/op_gestures_settings" android:fragment="com.oneplus.settings.OPGestureSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_display" android:id="@id/display_settings" android:title="@string/display_settings" android:fragment="com.android.settings_ex.DisplaySettings" />
        <dashboard-tile android:icon="@drawable/op_ic_sound_settings" android:id="@id/sound_settings" android:title="@string/op_sound_settings" android:fragment="com.android.settings_ex.notification.NotificationSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_notifications" android:id="@id/notification_settings" android:title="@string/op_do_not_disturb">
            <intent android:action="com.oneplus.notification.settings" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_battery" android:id="@id/battery_settings" android:title="@string/power_usage_summary_title" android:fragment="com.android.settings_ex.fuelgauge.PowerUsageSummary" />
        <dashboard-tile android:icon="@drawable/ic_settings_storage" android:id="@id/storage_settings" android:title="@string/op_storage" android:fragment="com.oneplus.settings.storage.OPStorageSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_memory" android:id="@id/manage_memory" android:title="@string/memory_settings_title" android:fragment="com.android.settings_ex.applications.ProcessStatsSummary" />
        <dashboard-tile android:icon="@drawable/ic_settings_multiuser" android:id="@id/user_settings" android:title="@string/user_settings_title" android:fragment="com.android.settings_ex.users.UserSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_nfc_payment" android:id="@id/nfc_payment_settings" android:title="@string/nfc_payment_settings_title" android:fragment="com.android.settings_ex.nfc.PaymentSettings" />
        <dashboard-tile android:id="@id/manufacturer_settings" android:fragment="com.android.settings_ex.WirelessSettings">
            <intent android:action="com.android.settings_ex.MANUFACTURER_APPLICATION_SETTING" />
        </dashboard-tile>
    </dashboard-category>
    <dashboard-category android:id="@id/personal_section" android:title="@string/header_category_personal" android:key="@string/category_key_personal">
        <dashboard-tile android:icon="@drawable/ic_settings_applications" android:id="@id/application_settings" android:title="@string/op_apps" android:fragment="com.android.settings_ex.applications.ManageApplications" />
        <dashboard-tile android:icon="@drawable/ic_settings_location" android:id="@id/location_settings" android:title="@string/location_settings_title" android:fragment="com.android.settings_ex.location.LocationSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_security" android:id="@id/security_settings" android:title="@string/op_fingerprint_settings" android:fragment="com.android.settings_ex.SecuritySettings" />
        <dashboard-tile android:icon="@drawable/op_ic_cloud_black" android:id="@id/cloud_service_settings" android:title="@string/op_cloud_service_title">
            <intent android:action="android.intent.action.ONEPLUSCLOUD" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_accounts" android:id="@id/account_settings" android:title="@string/account_settings_title" android:fragment="com.android.settings_ex.accounts.AccountSettings" />
        <external-tiles />
    </dashboard-category>
    <dashboard-category android:id="@id/system_section" android:title="@string/header_category_system" android:key="@string/category_key_system">
        <dashboard-tile android:icon="@drawable/ic_settings_date_time" android:id="@id/date_time_settings" android:title="@string/date_and_time_settings_title" android:fragment="com.android.settings_ex.DateTimeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_language" android:id="@id/language_settings" android:title="@string/language_settings" android:fragment="com.android.settings_ex.inputmethod.InputMethodAndLanguageSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_backup" android:id="@id/privacy_settings" android:title="@string/privacy_settings" android:fragment="com.android.settings_ex.PrivacySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_accessibility" android:id="@id/accessibility_settings" android:title="@string/accessibility_settings" android:fragment="com.android.settings_ex.accessibility.AccessibilitySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_print" android:id="@id/print_settings" android:title="@string/print_settings" android:fragment="com.android.settings_ex.print.PrintSettingsFragment" />
        <dashboard-tile android:icon="@drawable/op_ic_advanced_settings" android:id="@id/advanced_settings" android:title="@string/op_advanced_settings" android:fragment="com.oneplus.settings.others.OPOthersSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_development" android:id="@id/development_settings" android:title="@string/development_settings_title" android:fragment="com.android.settings_ex.DevelopmentSettings">
            <intent android:action="com.oneplus.noviceteaching.NoViceTeachingActivity" />
        </dashboard-tile>
        <external-tiles />
        <dashboard-tile android:icon="@drawable/ic_system_update" android:id="@id/system_updates" android:title="@string/op_system_update_settings">
            <intent android:action="android.intent.action.CheckUpdate" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_about" android:id="@id/about_settings" android:title="@string/about_settings" android:fragment="com.android.settings_ex.DeviceInfoSettings" />
    </dashboard-category>
</dashboard-categories>