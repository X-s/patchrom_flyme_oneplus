<?xml version="1.0" encoding="utf-8"?>
<dashboard-categories
  xmlns:android="http://schemas.android.com/apk/res/android">
    <dashboard-category android:id="@id/wireless_section" android:title="@string/header_category_wireless_networks">
        <dashboard-tile android:icon="@drawable/ic_settings_wireless" android:id="@id/wifi_settings" android:title="@string/wifi_settings_title" android:fragment="com.android.settings_ex.wifi.WifiSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_bluetooth2" android:id="@id/bluetooth_settings" android:title="@string/bluetooth_settings_title" android:fragment="com.android.settings_ex.bluetooth.BluetoothSettings" />
        <dashboard-tile android:icon="@drawable/ic_sim_sd" android:id="@id/sim_settings" android:title="@string/sim_settings_title" android:fragment="com.android.settings_ex.sim.SimSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_data_usage" android:id="@id/data_usage_settings" android:title="@string/data_usage_summary_title" android:fragment="com.android.settings_ex.DataUsageSummary" />
        <dashboard-tile android:id="@id/operator_settings" android:fragment="com.android.settings_ex.WirelessSettings">
            <intent android:action="com.android.settings_ex.OPERATOR_APPLICATION_SETTING" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_data_usage" android:id="@id/mobile_network_settings" android:title="@string/mobile_network_settings_title">
            <intent android:action="com.android.phone.MobileNetworkSettings" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_hotspot" android:id="@id/tether_settings" android:title="@string/tether_settings_title_all" android:fragment="com.android.settings_ex.TetherSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_sim" android:id="@id/simcard_manage_settings" android:title="@string/simcard_manage_settings_title" android:fragment="com.android.settings_ex.TetherSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_vpn" android:id="@id/vpn_settings" android:title="@string/vpn_settings_title" android:fragment="com.android.settings_ex.vpn2.VpnSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_more" android:id="@id/wireless_settings" android:title="@string/radio_controls_title" android:fragment="com.android.settings_ex.WirelessSettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/device_section" android:title="@string/header_display_sound_settings_title">
        <dashboard-tile android:icon="@drawable/ic_settings_buttons" android:id="@id/button_settings" android:title="@string/button_settings" android:fragment="com.android.settings_ex.ButtonSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_home" android:id="@id/home_settings" android:title="@string/home_settings" android:fragment="com.android.settings_ex.HomeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_wallpaper" android:id="@id/wallpaper" android:title="@string/wallpaper_settings_title" android:fragment="com.android.settings_ex.WallpaperTypeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_display" android:id="@id/display_settings" android:title="@string/display_settings" android:fragment="com.android.settings_ex.DisplaySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_date_time" android:id="@id/date_time_settings" android:title="@string/date_and_time_settings_title" android:fragment="com.android.settings_ex.DateTimeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_notifications" android:id="@id/notification_settings" android:title="@string/oneplus_notification_settings" android:fragment="com.android.settings_ex.notification.NotificationSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_storage" android:id="@id/storage_settings" android:title="@string/storage_settings" android:fragment="com.android.settings_ex.deviceinfo.Memory" />
        <dashboard-tile android:icon="@drawable/ic_settings_battery" android:id="@id/battery_settings" android:title="@string/power_usage_summary_title" android:fragment="com.android.settings_ex.fuelgauge.PowerUsageSummary" />
        <dashboard-tile android:icon="@drawable/ic_settings_multiuser" android:id="@id/user_settings" android:title="@string/user_settings_title" android:fragment="com.android.settings_ex.users.UserSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_nfc_payment" android:id="@id/nfc_payment_settings" android:title="@string/nfc_payment_settings_title" android:fragment="com.android.settings_ex.nfc.PaymentSettings" />
        <dashboard-tile android:id="@id/manufacturer_settings" android:fragment="com.android.settings_ex.WirelessSettings">
            <intent android:action="com.android.settings_ex.MANUFACTURER_APPLICATION_SETTING" />
        </dashboard-tile>
    </dashboard-category>
    <dashboard-category android:id="@id/private_safety_section" android:title="@string/private_safety_title">
        <dashboard-tile android:icon="@drawable/ic_settings_location" android:id="@id/location_settings" android:title="@string/location_settings_title" android:fragment="com.android.settings_ex.location.LocationSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_fingerprint" android:id="@id/fingerprint_password_manage_settings" android:title="@string/fingerprint_password_manage_title" android:fragment="com.android.settings_ex.FingerprintAndPWD" />
        <dashboard-tile android:icon="@drawable/ic_settings_applications_permisstion" android:id="@id/security_settings" android:title="@string/applications_permission_settings" android:fragment="com.android.settings_ex.SecuritySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_applications" android:id="@id/application_settings" android:title="@string/applications_permission_settings" android:fragment="com.android.settings_ex.applications.ManageApplicationsPermisstion" />
        <dashboard-tile android:icon="@drawable/ic_settings_backup" android:id="@id/sim_manage_settings" android:title="@string/sim_manage_settings_title">
            <intent android:targetPackage="com.android.settings_ex" android:action="android.intent.action.MAIN" android:targetClass="com.android.settings_ex.IccLockSettings" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_accounts" android:id="@id/account_settings" android:title="@string/account_settings_title" android:fragment="com.android.settings_ex.accounts.AccountSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_print" android:id="@id/truste_security_settings" android:title="@string/trusted_credentials" android:fragment="com.android.settings_ex.CredentialsManagementSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_print" android:id="@id/cloud_service_settings" android:title="@string/cloud_service_settings" android:fragment="com.android.settings_ex.cloud.CloudServiceSettings" />
    </dashboard-category>
    <dashboard-category android:id="@id/system_section" android:title="@string/header_other_settings_title">
        <dashboard-tile android:icon="@drawable/ic_settings_cn_language" android:id="@id/language_settings" android:title="@string/language_settings" android:fragment="com.android.settings_ex.inputmethod.InputMethodAndLanguageSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_accessibility" android:id="@id/accessibility_settings" android:title="@string/accessibility_settings" android:fragment="com.android.settings_ex.accessibility.AccessibilitySettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_print" android:id="@id/print_settings" android:title="@string/print_settings" />
        <dashboard-tile android:icon="@drawable/ic_settings_development" android:id="@id/development_settings" android:title="@string/development_settings_title" android:fragment="com.android.settings_ex.DevelopmentSettings" />
        <dashboard-tile android:icon="@drawable/op_ic_settings_development" android:id="@id/advance_development_settings" android:title="@string/advance_dev_settings_title" android:fragment="com.android.settings_ex.AdvanceDevSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_about" android:id="@id/about_settings" android:title="@string/about_settings" android:fragment="com.android.settings_ex.DeviceInfoSettings" />
    </dashboard-category>
</dashboard-categories>