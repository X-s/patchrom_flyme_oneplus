<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/advance_dev_settings_title"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings_ex">
    <com.oneplus.widget.preference.OPPreferenceCategory android:title="@string/oneplus_advance_dev_settings_title" android:key="advance_dev_settings">
        <PreferenceScreen android:title="@string/left_button_customization_enable_title" android:key="left_button_customization" android:fragment="com.android.settings_ex.accessibility.KeyCustomizeFragements" />
        <PreferenceScreen android:title="@string/black_screen_gestures_settings_title" android:key="black_screen_gestures_settings" android:fragment="com.android.settings_ex.accessibility.BlackScreenPreferenceFragement" />
        <PreferenceScreen android:title="@string/timer_shutdown_startup_settings_title" android:key="timer_shutdown_startup_settings" android:fragment="com.oneplus.settings.timer.timepower.TimepowerSettingsFragment" />
        <PreferenceScreen android:title="@string/motion_sensor_settings_title" android:key="motion_sensor_settings" android:fragment="com.android.settings_ex.sensor.MotionSensorSettings" />
    </com.oneplus.widget.preference.OPPreferenceCategory>
    <com.android.settings_ex.ui.OPPreferenceDivider android:key="preference_divider_line" />
    <com.oneplus.widget.preference.OPPreferenceCategory android:title="@string/oneplus_advance_dev_others_settings_title" android:key="advance_dev_others_settings">
        <com.oneplus.widget.preference.OPPreference android:title="@string/power_usage_summary_title" android:key="battery_settings" android:fragment="com.android.settings_ex.fuelgauge.PowerUsageSummary" />
        <com.oneplus.widget.preference.OPPreference android:title="@string/storage_settings" android:key="storage_settings" android:fragment="com.android.settings_ex.deviceinfo.Memory" />
        <com.oneplus.widget.preference.OPSwitchPreference android:title="@string/otg_read_enable" android:key="otg_read_enable" />
        <com.oneplus.widget.preference.OPSwitchPreference android:title="@string/notify_light_enable_title" android:key="notify_light_enable" />
        <com.oneplus.widget.preference.OPSwitchPreference android:title="@string/accessibility_power_button_ends_call_prerefence_title" android:key="toggle_power_button_ends_call_preference" />
        <com.oneplus.widget.preference.OPSwitchPreference android:title="@string/oneplus_user_enjoy_plan_title" android:key="user_enjoy_plan" />
        <com.oneplus.widget.preference.OPPreference android:title="@string/trusted_credentials" android:key="truste_security_settings" android:fragment="com.android.settings_ex.CredentialsManagementSettings" />
        <com.oneplus.widget.preference.OPPreference android:title="@string/development_settings_title" android:key="development_settings" android:fragment="com.android.settings_ex.DevelopmentSettings" />
        <com.oneplus.widget.preference.OPPreference android:id="@id/accessibility_settings" android:title="@string/accessibility_settings" android:fragment="com.oneplus.settings.accessibility.AccessibilitySettings" />
        <com.oneplus.widget.preference.OPPreference android:id="@id/privacy_settings" android:title="@string/reset_settings" android:fragment="com.android.settings_ex.PrivacySettings" />
    </com.oneplus.widget.preference.OPPreferenceCategory>
</PreferenceScreen>