<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/privacy_settings_title"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings_ex">
    <com.oneplus.settings.ui.OPPreferenceHeaderMargin android:key="preference_divider_line" />
    <PreferenceScreen android:persistent="false" android:title="@string/backup_data_title" android:key="backup_data" android:fragment="com.android.settings_ex.backup.ToggleBackupSettingFragment" />
    <PreferenceScreen android:persistent="false" android:title="@string/backup_configure_account_title" android:key="configure_account">
        <intent android:action="dummy" />
    </PreferenceScreen>
    <PreferenceScreen android:persistent="false" android:key="data_management" />
    <SwitchPreference android:title="@string/auto_restore_title" android:key="auto_restore" android:summary="@string/auto_restore_summary" />
    <Preference android:persistent="false" android:enabled="false" android:title="@string/backup_inactive_title" android:selectable="false" android:key="backup_inactive" />
    <com.android.settings_exlib.RestrictedPreference android:title="@string/reset_network_title" android:key="network_reset" android:fragment="com.android.settings_ex.ResetNetwork" settings:userRestriction="no_network_reset" settings:useAdminDisabledSummary="true" settings:keywords="@string/keywords_network_reset" />
    <com.android.settings_exlib.RestrictedPreference android:title="@string/master_clear_title" android:key="factory_reset" android:fragment="com.android.settings_ex.MasterClear" settings:userRestriction="no_factory_reset" settings:useAdminDisabledSummary="true" settings:keywords="@string/keywords_factory_data_reset" />
    <PreferenceScreen android:title="@string/collect_diagnostics_title" android:key="collect_diagnostics" android:summary="@string/collect_diagnostics_summary">
        <intent android:targetPackage="com.tmobile.pr.mytmobile" android:action="com.metrics.tmobile.SUMMARY" android:targetClass="com.tmobile.pr.mytmobile.iqtoggle.ui.OptInSummary" />
    </PreferenceScreen>
</PreferenceScreen>