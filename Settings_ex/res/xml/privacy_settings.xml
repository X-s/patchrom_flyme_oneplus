<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/privacy_settings_title"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings_ex">
    <PreferenceCategory android:title="@string/backup_section_title" android:key="backup_category">
        <SwitchPreference android:persistent="false" android:title="@string/backup_data_title" android:key="backup_data" android:summary="@string/backup_data_summary" />
        <PreferenceScreen android:persistent="false" android:title="@string/backup_configure_account_title" android:key="configure_account" android:summary="@string/backup_configure_account_default_summary">
            <intent android:action="dummy" />
        </PreferenceScreen>
        <SwitchPreference android:persistent="false" android:title="@string/auto_restore_title" android:key="auto_restore" android:summary="@string/auto_restore_summary" />
        <Preference android:persistent="false" android:enabled="false" android:title="@string/backup_inactive_title" android:selectable="false" android:key="backup_inactive" android:summary="@string/backup_inactive_summary" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/personal_data_section_title" android:key="personal_data_category">
        <Preference android:title="@string/clear_all_settings_title" android:key="clear_all_settings" android:summary="@string/clear_all_settings_summary" />
        <PreferenceScreen android:title="@string/master_clear_title" android:key="factory_data_reset" android:summary="@string/master_clear_summary" android:fragment="com.android.settings_ex.MasterClear" settings:keywords="@string/keywords_factory_data_reset" />
        <Preference android:title="@string/oneplus_setting_factory_password_title" android:key="setting_factory_password" android:summary="@string/oneplus_setting_factory_password_summary" />
    </PreferenceCategory>
</PreferenceScreen>