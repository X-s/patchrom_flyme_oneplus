<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/radio_controls_title"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings_ex">
    <com.oneplus.settings.ui.OPPreferenceHeaderMargin android:key="preference_divider_line" />
    <SwitchPreference android:title="@string/airplane_mode" android:key="toggle_airplane" android:disableDependentsState="true" />
    <com.android.settings_exlib.RestrictedPreference android:title="@string/tether_settings_title_all" android:key="tether_settings" android:fragment="com.android.settings_ex.TetherSettings" />
    <com.android.settings_exlib.RestrictedPreference android:title="@string/vpn_settings_title" android:key="vpn_settings" android:fragment="com.android.settings_ex.vpn2.VpnSettings" settings:userRestriction="no_config_vpn" settings:useAdminDisabledSummary="true" />
    <PreferenceScreen android:title="@string/cell_broadcast_settings" android:key="cell_broadcast_settings">
        <intent android:targetPackage="com.android.cellbroadcastreceiver" android:action="android.intent.action.MAIN" android:targetClass="com.android.cellbroadcastreceiver.CellBroadcastSettings" />
    </PreferenceScreen>
    <com.oneplus.settings.ui.OPPreferenceDivider android:key="preference_divider_line" />
    <PreferenceCategory android:layout="@layout/op_preference_category_material" android:title="@string/nfc_quick_toggle_title" android:key="nfc_key">
        <SwitchPreference android:persistent="false" android:title="@string/nfc_quick_toggle_title" android:key="toggle_nfc" android:summary="@string/nfc_quick_toggle_summary" />
        <com.android.settings_exlib.RestrictedPreference android:title="@string/android_beam_settings_title" android:key="android_beam_settings" android:fragment="com.android.settings_ex.nfc.AndroidBeam" />
        <PreferenceScreen android:title="@string/oneplus_nfc_payment_settings_title" android:key="android_payment_settings" android:fragment="com.android.settings_ex.nfc.PaymentSettings" />
    </PreferenceCategory>
    <PreferenceScreen android:title="@string/wifi_calling_settings_title" android:key="wifi_calling_settings" android:fragment="com.android.settings_ex.WifiCallingSettings" settings:keywords="@string/keywords_wifi_calling" />
    <PreferenceScreen android:persistent="false" android:title="@string/wifi_calling_settings_title" android:key="wifi_calling_enhanced_settings" android:summaryOn="@null" android:summaryOff="@null">
        <intent android:targetPackage="com.qualcomm.qti.wfcservice" android:action="android.intent.action.MAIN" android:targetClass="com.qualcomm.qti.wfcservice.WifiCallingEnhancedSettings" />
    </PreferenceScreen>
    <com.android.settings_exlib.RestrictedPreference android:title="@string/network_settings_title" android:key="mobile_network_settings" android:dependency="toggle_airplane" settings:userRestriction="no_config_mobile_networks" settings:useAdminDisabledSummary="true" settings:keywords="@string/keywords_more_mobile_networks">
        <intent android:targetPackage="com.android.phone" android:action="android.intent.action.MAIN" android:targetClass="com.android.phone.MobileNetworkSettings" />
    </com.android.settings_exlib.RestrictedPreference>
    <com.android.settings_exlib.RestrictedPreference android:persistent="false" android:title="@string/manage_mobile_plan_title" android:key="manage_mobile_plan" settings:userRestriction="no_config_mobile_networks" settings:useAdminDisabledSummary="true" />
    <com.android.settings_exlib.RestrictedPreference android:title="@string/wimax_settings" android:key="wimax_settings" settings:userRestriction="no_config_mobile_networks" settings:useAdminDisabledSummary="true">
        <intent android:targetPackage="com.android.settings_ex.wimax" android:action="android.intent.action.MAIN" android:targetClass="com.android.settings_ex.wimax.WimaxSettings" />
    </com.android.settings_exlib.RestrictedPreference>
    <PreferenceScreen android:title="@string/proxy_settings_title" android:key="proxy_settings" android:fragment="com.android.settings_ex.ProxySelector" />
    <SwitchPreference android:title="@string/voice_over_lte" android:key="voice_over_lte" />
</PreferenceScreen>