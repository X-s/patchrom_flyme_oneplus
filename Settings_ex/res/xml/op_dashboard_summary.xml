<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/dashboard_title" android:key="settings"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <com.oneplus.widget.preference.OPPreferenceCategory android:title="@string/header_category_wireless_networks" android:key="wireless_section">
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_wireless" android:title="@string/wifi_settings_title" android:key="wifi_settings" android:fragment="com.android.settings_ex.wifi.WifiSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_bluetooth" android:title="@string/bluetooth_settings_title" android:key="bluetooth_settings" android:fragment="com.android.settings_ex.bluetooth.BluetoothSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_settings_sim" android:title="@string/op_sim_cord" android:key="sim_settings_and_mobile_network_settings">
            <intent android:action="oneplus.intent.action.SIM_AND_NETWORK_SETTINGS" />
        </com.oneplus.settings.OPDashboardPreference>
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_data_usage" android:title="@string/data_usage_summary_title" android:key="mobile_network_settings">
            <intent android:action="com.oneplus.security.action.USAGE_DATA_SUMMARY" />
        </com.oneplus.settings.OPDashboardPreference>
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_more" android:title="@string/radio_controls_title" android:key="wireless_settings" android:fragment="com.android.settings_ex.WirelessSettings" />
    </com.oneplus.widget.preference.OPPreferenceCategory>
    <com.android.settings_ex.ui.OPPreferenceThinDivider android:key="preference_divider_line" />
    <com.oneplus.widget.preference.OPPreferenceCategory android:title="@string/header_category_device" android:key="device_section">
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_do_not_disturb" android:title="@string/oneplus_three_key_zenmod" android:key="zen_mode" android:fragment="com.oneplus.settings.notification.OPNotificationAndNotdisturb" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_buttons" android:title="@string/op_buttons_settings" android:key="buttons_settings" android:fragment="com.oneplus.settings.OPButtonsSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_gestures" android:title="@string/op_gestures_settings" android:key="gestures_settings" android:fragment="com.oneplus.settings.OPGestureSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_display" android:title="@string/display_settings" android:key="display_settings" android:fragment="com.android.settings_ex.DisplaySettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_sound_settings" android:title="@string/op_sound_settings" android:key="sound_settings" android:fragment="com.android.settings_ex.notification.NotificationSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_notifications" android:title="@string/op_do_not_disturb" android:key="notification_settings">
            <intent android:action="com.oneplus.notification.settings" />
        </com.oneplus.settings.OPDashboardPreference>
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_battery" android:title="@string/power_usage_summary_title" android:key="battery_settings" android:fragment="com.android.settings_ex.fuelgauge.PowerUsageSummary" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_storage" android:title="@string/op_storage" android:key="storage_settings" android:fragment="com.oneplus.settings.storage.OPStorageSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_multiuser" android:title="@string/user_settings_title" android:key="user_settings" android:fragment="com.android.settings_ex.users.UserSettings" />
    </com.oneplus.widget.preference.OPPreferenceCategory>
    <com.android.settings_ex.ui.OPPreferenceThinDivider android:key="preference_divider_line" />
    <com.oneplus.widget.preference.OPPreferenceCategory android:title="@string/header_category_personal" android:key="personal_section">
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_applications" android:title="@string/op_apps" android:key="application_settings" android:fragment="com.android.settings_ex.applications.ManageApplications" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_location" android:title="@string/location_settings_title" android:key="location_settings" android:fragment="com.android.settings_ex.location.LocationSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_security" android:title="@string/op_fingerprint_settings" android:key="security_settings" android:fragment="com.android.settings_ex.SecuritySettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_cloud_black" android:title="@string/op_cloud_service_title" android:key="cloud_service_settings">
            <intent android:action="android.intent.action.ONEPLUSCLOUD" />
        </com.oneplus.settings.OPDashboardPreference>
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_accounts" android:title="@string/account_settings_title" android:key="account_settings" android:fragment="com.android.settings_ex.accounts.AccountSettings" />
        <com.oneplus.settings.OPDashboardPreference android:key="google_gms" />
    </com.oneplus.widget.preference.OPPreferenceCategory>
    <com.android.settings_ex.ui.OPPreferenceThinDivider android:key="preference_divider_line" />
    <com.oneplus.widget.preference.OPPreferenceCategory android:title="@string/header_category_system" android:key="system_section">
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_date_time" android:title="@string/date_and_time_settings_title" android:key="date_time_settings" android:fragment="com.android.settings_ex.DateTimeSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_language" android:title="@string/language_settings" android:key="language_settings" android:fragment="com.android.settings_ex.inputmethod.InputMethodAndLanguageSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_backup" android:title="@string/privacy_settings" android:key="privacy_settings" android:fragment="com.android.settings_ex.PrivacySettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_advanced_settings" android:title="@string/op_advanced_settings" android:key="advanced_settings" android:fragment="com.oneplus.settings.others.OPOthersSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_development" android:title="@string/development_settings_title" android:key="development_settings" android:fragment="com.android.settings_ex.DevelopmentSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_development" android:title="@string/development_settings_title" android:key="oneplus_lab">
            <intent android:action="android.intent.action.ONEPLUS_LAB_FEATURE" />
        </com.oneplus.settings.OPDashboardPreference>
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_system_update" android:title="@string/op_system_update_settings" android:key="system_updates">
            <intent android:action="android.intent.action.CheckUpdate" />
        </com.oneplus.settings.OPDashboardPreference>
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_about" android:title="@string/about_settings" android:key="about_settings" android:fragment="com.android.settings_ex.DeviceInfoSettings" />
    </com.oneplus.widget.preference.OPPreferenceCategory>
</PreferenceScreen>