<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/dashboard_title" android:key="settings"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <com.oneplus.widget.preference.OPPreferenceCategory android:title="@string/oneplus_header_category_wireless_networks" android:key="wireless_section">
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_wireless" android:title="@string/oneplus_wifi_settings_title" android:key="wifi_settings" android:fragment="com.android.settings_ex.wifi.WifiSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_bluetooth2" android:title="@string/oneplus_bluetooth_settings_title" android:key="bluetooth_settings" android:fragment="com.android.settings_ex.bluetooth.BluetoothSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_double_crad_manager" android:title="@string/oneplus_sim_and_mobile_settings_title" android:key="sim_settings_and_mobile_network_settings">
            <intent android:action="oneplus.intent.action.SIM_AND_NETWORK_SETTINGS" />
        </com.oneplus.settings.OPDashboardPreference>
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_settings_more" android:title="@string/oneplus_radio_controls_title" android:key="wireless_settings" android:fragment="com.android.settings_ex.WirelessSettings" />
    </com.oneplus.widget.preference.OPPreferenceCategory>
    <com.android.settings_ex.ui.OPPreferenceDivider android:key="preference_divider_line" />
    <com.oneplus.widget.preference.OPPreferenceCategory android:title="@string/oneplus_header_display_sound_settings_title" android:key="device_section">
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_zenmode" android:title="@string/oneplus_three_key_zenmod" android:key="zen_mode" android:fragment="com.oneplus.settings.notification.OPThreeKeyAndZenmode" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_notifications" android:title="@string/oneplus_notification_settings" android:key="notification_settings" android:fragment="com.oneplus.settings.notification.OPNotificationSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_display" android:title="@string/oneplus_display_settings" android:key="display_settings" android:fragment="com.oneplus.settings.OPDisplaySettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_zenmode" android:title="@string/oneplus_zen_mode_settings_title" android:key="no_disturb_mode" android:fragment="com.oneplus.settings.notification.OPZenModeSettings" />
    </com.oneplus.widget.preference.OPPreferenceCategory>
    <com.android.settings_ex.ui.OPPreferenceDivider android:key="preference_divider_line" />
    <com.oneplus.widget.preference.OPPreferenceCategory android:title="@string/oneplus_private_safety_title" android:key="private_safety_section">
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_location" android:title="@string/oneplus_location_settings_title" android:key="location_settings" android:fragment="com.android.settings_ex.location.LocationSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_fingerprint" android:title="@string/oneplus_fingerprint_password_manage_title" android:key="fingerprint_settings" android:fragment="com.oneplus.settings.security.OPSecuritySettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_fingerprint" android:title="@string/oneplus_sub_fingerprint_password_manage_title" android:key="password_manage_settings" android:fragment="com.oneplus.settings.security.OPSecuritySettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_notification_manager" android:title="@string/application_notification_title" android:key="noification_settings" android:fragment="com.oneplus.settings.notification.OPNotificationAppList" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_applications_permisstion" android:title="@string/oneplus_applications_permission_settings" android:key="security_settings" android:fragment="com.oneplus.settings.security.OPApplicationSecuritySettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_accounts" android:title="@string/oneplus_account_settings_title" android:key="account_settings" android:fragment="com.android.settings_ex.accounts.AccountSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_settings_backup" android:title="@string/master_clear_title" android:key="privacy_settings" android:fragment="com.android.settings_ex.PrivacySettings" />
    </com.oneplus.widget.preference.OPPreferenceCategory>
    <com.android.settings_ex.ui.OPPreferenceDivider android:key="preference_divider_line" />
    <com.oneplus.widget.preference.OPPreferenceCategory android:title="@string/oneplus_header_other_settings_title" android:key="system_section">
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_intelligent_assistant" android:title="@string/oneplus_iq_settings_title" android:key="iq_acc_settings" android:fragment="com.oneplus.settings.iq.OPIQSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_settings_battery" android:title="@string/power_usage_summary_title" android:key="battery_settings" android:fragment="com.android.settings_ex.fuelgauge.PowerUsageSummary" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_date_time" android:title="@string/oneplus_date_and_time_settings_title" android:key="date_time_settings" android:fragment="com.android.settings_ex.DateTimeSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_cn_language" android:title="@string/oneplus_language_settings" android:key="language_settings" android:fragment="com.android.settings_ex.inputmethod.InputMethodAndLanguageSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_other_settings" android:title="@string/oneplus_others_settings_title" android:key="onepluse_other_development_settings" android:fragment="com.oneplus.settings.others.OPOthersSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/oneplus_noviceguide" android:title="@string/oneplus_noviceguide" android:key="op_noviceguide">
            <intent android:action="com.oneplus.noviceteaching.NoViceTeachingActivity" />
        </com.oneplus.settings.OPDashboardPreference>
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/op_ic_update_settings" android:title="@string/system_update_settings_list_item_title" android:key="onepluse_system_upgrade_settings">
            <intent android:action="android.intent.action.CheckUpdate" />
        </com.oneplus.settings.OPDashboardPreference>
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_development" android:title="@string/oneplus_development_settings_title" android:key="development_settings" android:fragment="com.android.settings_ex.DevelopmentSettings" />
        <com.oneplus.settings.OPDashboardPreference android:icon="@drawable/ic_settings_about" android:title="@string/oneplus_about_settings" android:key="about_settings" android:fragment="com.oneplus.settings.OPDeviceInfoSettings" />
    </com.oneplus.widget.preference.OPPreferenceCategory>
</PreferenceScreen>