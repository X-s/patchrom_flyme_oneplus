<?xml version="1.0" encoding="utf-8"?>
<animated-selector android:constantSize="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:id="@id/off" android:drawable="@drawable/watch_switch_thumb_material" />
    <item android:state_checked="true" android:id="@id/on" android:drawable="@drawable/watch_switch_thumb_material" />
    <item android:id="@id/off" android:drawable="@drawable/watch_switch_thumb_material" />
    <transition android:drawable="@drawable/watch_switch_thumb_to_on_anim_mtrl" android:toId="@id/on" android:fromId="@id/off" />
    <transition android:drawable="@drawable/watch_switch_thumb_to_off_anim_mtrl" android:toId="@id/off" android:fromId="@id/on" />
</animated-selector>