<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <nine-patch android:src="@drawable/uc_switch_track_mtrl_alpha" android:tint="?android:colorForeground" android:alpha="0.1" />
    </item>
    <item android:state_checked="true">
        <nine-patch android:src="@drawable/uc_switch_track_mtrl_alpha" android:tint="?android:colorControlActivated" android:alpha="0.3" />
    </item>
    <item>
        <nine-patch android:src="@drawable/uc_switch_track_mtrl_alpha" android:tint="?android:colorForeground" android:alpha="0.3" />
    </item>
</selector>